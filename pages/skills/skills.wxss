.icon-gonglve:before {
  content: "\e73b";
}
.icon-yingxiong:before {
  content: "\e845";
}
page {
  background-color: #F1F1F1;
}
.container .userinfo-msg {
  display: flex;
  justify-content: start;
  align-items: center;
  height: 60rpx;
  background-color: white;
}
.container .userinfo-msg .userAvatar {
  overflow: hidden;
  width: 60rpx;
  border-radius: 50%;
}
.container .nav {
  display: flex;
  background-color: #db9e3f;
}
.container .nav .nav-center {
  flex: 1;
  text-align: center;
  background-color: #db9e3f;
  color: white;
  font-size: 30rpx;
  height: 70rpx;
  padding: 0;
  line-height: 70rpx;
  min-height: 70rpx;
}
.container .nav .nav-center .text-bb {
  border-bottom: 4rpx solid white;
  font-weight: bold;
  line-height: 70rpx;
}
.container .hero-skills {
  background-color: white;
  padding: 0 30rpx;
  margin-top: 30rpx;
  border-top: 2rpx solid transparent;
  position: relative;
}
.container .hero-skills .hero-title {
  margin: 20rpx 0;
  border-bottom: 2rpx solid black;
  padding: 10rpx 0;
}
.container .hero-skills .hero .hero-hot {
  display: flex;
  justify-content: space-between;
  width: 100%;
  padding-bottom: 20rpx;
}
.container .hero-skills .hero .hreo-img {
  width: 100rpx;
  height: 100rpx;
  background-size: cover;
  display: flex;
  flex-direction: column;
  justify-content: flex-end;
  align-items: center;
}
.container .hero-skills .hero .hreo-img .hero-classify {
  background-color: rgba(0, 0, 0, 0.3);
  font-size: 24rpx;
  color: white;
}
.container .hero-skills .hero .hero-more {
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  background-color: #f1f1f1;
}
.container .hero-skills .morehero {
  position: absolute;
  bottom: 0;
  width: 100%;
  right: -30rpx;
  transform: translateY(105%);
  display: flex;
  justify-content: space-between;
  background-color: white;
  border: 2rpx solid #f1f1f1;
  margin-top: 30rpx;
  margin: 30rpx;
}
.container .hero-skills .morehero .moerhero-hero {
  flex: 4;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  padding: 30rpx;
}
.container .hero-skills .morehero .moerhero-hero .hero-item {
  margin-right: 20rpx;
  text-align: center;
}
.container .hero-skills .morehero .moerhero-hero .more-item {
  width: 110rpx;
  height: 110rpx;
  background-size: cover;
}
.container .hero-skills .morehero .morehero-classify {
  flex: 1;
  display: flex;
  flex-direction: column;
  height: 100%;
}
.container .hero-skills .morehero .morehero-classify .classify {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: black;
  font-size: 30rpx;
}
.container .hero-skills .morehero .morehero-classify .active {
  color: white;
  background-color: #db9e3f;
}
.container .hot-skills {
  margin-top: 20rpx;
  padding: 30rpx;
  background-color: white;
}
.container .hot-skills .hs-head {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding-bottom: 20rpx;
  border-bottom: 2rpx solid black;
}
.container .hot-skills .hs-head .icon {
  display: inline-block;
  margin-top: 6rpx;
}
.container .hot-skills .hs-head .hs-title {
  margin-left: 10rpx;
  vertical-align: baseline;
}
.container .hot-skills .hs-items {
  display: flex;
  height: 140rpx;
  justify-content: space-between;
  margin-top: 30rpx;
}
.container .hot-skills .hs-items .hst-img {
  width: 260rpx;
}
.container .hot-skills .hs-items .hst-description {
  height: 140rpx;
  width: 410rpx;
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.container .hot-skills .hs-items .hst-description .hst-title {
  width: 100%;
  font-size: 24rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.container .hot-skills .hs-items .hst-description .hst-de {
  width: 100%;
  font-size: 22rpx;
  color: gray;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
}
